{
  "expo": {
    "name": "VideoFolder",
    "slug": "VideoFolder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./source/assets/appIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./source/assets/appIconSplash.png",
      "alignSelf": "center",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./source/assets/appIcon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./source/assets/appIcon.png"
    }
  }
}
